{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "base": {
      "node": "16.12.0"
    },
    "development": {
      "extends": "base",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_BASE_URL": "https://www.taxistandil.online"
        }
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": "true",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_PUBLIC_API_BASE_URL": "https://www.taxistandil.online"
        }
      }
    },
    "preview": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview2": {
      "developmentClient": true
    },
    "preview3": {
      "distribution": "internal"
    }
  }
}
