{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "base": {
      "node": "16.12.0"
    },
    "development": {
      "extends": "base",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_API_BASE_URL": "http://taxistandil.online:80",
          "EXPO_PUBLIC_WS_URL": "http://taxistandil.online:2000"
        }
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": "true",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_PUBLIC_API_BASE_URL": "https://taxistandil.online:80",
          "EXPO_PUBLIC_WS_URL": "https://taxistandil.online:2000"
        }
      }
    },
    "preview": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview2": {
      "developmentClient": true
    },
    "preview3": {
      "distribution": "internal"
    }
  }
}
