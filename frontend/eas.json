{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "base": {
      "node": "18.20.3"
    },
    "dev-aws-url": {
      "extends": "base",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_BASE_URL": "https://www.taxistandil.online"
        }
      },
      "ios": {
        "simulator": true
      }
    },
    "dev-local-url": {
      "extends": "dev-aws-url",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_BASE_URL": "https://glowworm-light-infinitely.ngrok-free.app"
        }
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": "true",
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://www.taxistandil.online"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production"
    },
    "staging": {
      "extends": "base",
      "autoIncrement": "true",
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://www.taxistandil.online"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "staging"
    }
  }
}
